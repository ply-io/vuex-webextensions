{
  "name": "@ply-io/vuex-webextensions",
  "version": "1.3.3",
  "description": "Use Vuex on WebExtensions on 'shared' context - Ply FORK",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && babel src -d dist",
    "lint": "eslint src",
    "precommit": "npm run lint && pretty-quick --staged",
    "prepare": "npm run lint && npm run test && npm run build",
    "prettier": "prettier **/*.{js,json,css,vue}",
    "prettier:write": "npm run prettier -- --write",
    "test": "echo \"Tests aren't ready for now\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ply-io/vuex-webextensions.git"
  },
  "keywords": [
    "vuex",
    "webextension",
    "node",
    "npm",
    "firefox",
    "chrome"
  ],
  "author": "Mitsuha Kitsune <kitsunemitsuha@gmail.com> (https://mitsuhakitsune.com) / Mods by Ply team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ply-io/vuex-webextensions/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-async-to-generator": "^7.7.4",
    "@babel/plugin-transform-flow-strip-types": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "babel-eslint": "^8.2.6",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^0.14.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "rimraf": "^2.7.1",
    "rollup": "^1.27.9",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-uglify": "^6.0.3"
  },
  "peerDependencies": {
    "vue": ">=2.0.0",
    "vuex": ">=2.0.0"
  }
}
